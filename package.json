{
  "name": "umi-plugin-mpa",
  "version": "0.1.1",
  "description": "MPA(multiple-page application) plugin for umi.",
  "repository": "umijs/umi-plugin-mpa",
  "author": {
    "name": "chencheng",
    "email": "sorrycc@gmail.com"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "umi",
    "umi-plugin",
    "mpa"
  ],
  "scripts": {
    "changelog": "git changelog CHANGELOG.md && git add ./ && git commit -m 'update CHANGELOG.md'",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run changelog && np --no-cleanup --yolo --no-publish",
    "test": "umi-test"
  },
  "dependencies": {
    "deasync-promise": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.1",
    "is-plain-object": "^2.0.4",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "np": "3",
    "prettier": "^1.15.3",
    "test-build-result": "^1.0.1",
    "umi": "2",
    "umi-test": "1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "templates",
    "index.js"
  ],
  "license": "MIT"
}
