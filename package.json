{
  "name": "umi-plugin-mpa",
  "version": "0.1.1",
  "description": "MPA(multiple-page application) plugin for umi.",
  "repository": "umijs/umi-plugin-mpa",
  "main": "dist/index.js",
  "author": {
    "name": "chencheng",
    "email": "sorrycc@gmail.com"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "umi",
    "umi-plugin",
    "mpa"
  ],
  "scripts": {
    "build": "rollup -c",
    "changelog": "git changelog CHANGELOG.md && git add ./ && git commit -m 'update CHANGELOG.md'",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run changelog && np --no-cleanup --yolo --no-publish",
    "test": "umi-test"
  },
  "dependencies": {
    "deasync-promise": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^6.2.1",
    "is-plain-object": "^2.0.4",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@types/node": "^10.12.18",
    "coveralls": "^3.0.2",
    "execa": "^1.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "np": "3",
    "prettier": "^1.15.3",
    "rollup": "^1.1.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "test-build-result": "^1.0.1",
    "umi": "2",
    "umi-plugin-types": "0.2.1",
    "umi-test": "1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "templates",
    "src",
    "dist"
  ],
  "license": "MIT"
}
